msgstr "Vigsel"

#: templates/maps.html:7
msgid "WEDDING_LOCATION"
msgstr "Logen på Kyrkholmen, Skellefteå"

#: templates/maps.html:12
msgid "Reception"
msgstr "Mottagning"

#: templates/maps.html:15
msgid "RECEPTION_LOCATION"
msgstr "Stiftsgården Skellefteå"

